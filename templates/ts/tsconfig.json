{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
        "@titan/native": ["./app/titan"],
        "@titan/route": ["./titan/titan"],
        "*": ["./app/*"]
    }
  },
  "include": ["app/**/*", "titan/**/*"]
}
